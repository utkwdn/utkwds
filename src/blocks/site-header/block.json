{
	"$schema": "https://schemas.wp.org/trunk/block.json",
	"apiVersion": 2,
	"name": "utk-wds/site-header",
	"version": "0.1.0",
	"title": "Universal Site Header",
	"category": "theme",
	"icon": "smiley",
	"description": "Header for the top of every page on the site",
	"supports": {
		"html": true,
    "inserter": false
	},
	"attributes": {
		"mainMenuName": {
			"type": "string"
		},
		"utilityMenuName": {
			"type": "string"
		}
	},
	"textdomain": "utk-wds",
	"editorScript": "file:./index.js",
	"editorStyle": "file:./index.css",
	"style": ["file:../nav-menu/style-index.css", "file:./style-index.css"],
	"render": "file:./render.php"
}

{
  "$schema": "https://playground.wordpress.net/blueprint-schema.json",
  "extraLibraries": [
    "wp-cli"
  ],
  "steps": [
    {
      "step": "setSiteOptions",
      "options": {
        "blogname": "Rocky Block"
      }
    },
    {
      "step": "installTheme",
      "themeData": {
        "resource": "url",
        "url": "https://github.com/utkwdn/utkwds/releases/download/v1.2.4/utkwds.zip"
      }
    },
    {
      "step": "wp-cli",
      "command": "wp utkwds kitchen-sink add"
    }
  ]
}
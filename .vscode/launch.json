  "version": "0.2.0",
  "configurations": [
    {
      "name": "Listen for Xdebug",
      "type": "php",
      "request": "launch",
      "port": 9003,
      "pathMappings": {
        "/var/www/html/wp-content/themes/utkwds": "${workspaceFolder}/src",
        "/var/www/html/wp-admin": "${env.WP_CORE_PATH}/wp-admin",
        "/var/www/html/wp-includes": "${env.WP_CORE_PATH}/wp-includes",
        "/var/www/html/index.php": "${env.WP_CORE_PATH}/index.php",
        "/var/www/html/wp-activate.php": "${env.WP_CORE_PATH}/wp-activate.php",
        "/var/www/html/wp-blog-header.php": "${env.WP_CORE_PATH}/wp-blog-header.php",
        "/var/www/html/wp-comments-post.php": "${env.WP_CORE_PATH}/wp-comments-post.php",
        "/var/www/html/wp-config-sample.php": "${env.WP_CORE_PATH}/wp-config-sample.php",
        "/var/www/html/wp-config.php": "${env.WP_CORE_PATH}/wp-config.php",
        "/var/www/html/wp-cron.php": "${env.WP_CORE_PATH}/wp-cron.php",
        "/var/www/html/wp-links-opml.php": "${env.WP_CORE_PATH}/wp-links-opml.php",
        "/var/www/html/wp-load.php": "${env.WP_CORE_PATH}/wp-load.php",
        "/var/www/html/wp-login.php": "${env.WP_CORE_PATH}/wp-login.php",
        "/var/www/html/wp-mail.php": "${env.WP_CORE_PATH}/wp-mail.php",
        "/var/www/html/wp-settings.php": "${env.WP_CORE_PATH}/wp-settings.php",
        "/var/www/html/wp-signup.php": "${env.WP_CORE_PATH}/wp-signup.php",
        "/var/www/html/wp-trackback.php": "${env.WP_CORE_PATH}/wp-trackback.php",
        "/var/www/html/xmlrpc.php": "${env.WP_CORE_PATH}/xmlrpc.php"
      }
    }
  ]
}